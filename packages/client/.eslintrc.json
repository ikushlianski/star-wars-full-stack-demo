{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended",
    "../../.eslintrc.json"
  ],
  "plugins": [
    "@typescript-eslint",
    "prettier",
    "react-hooks"
  ],
  "ecmaFeatures": {
    "jsx": true
  },
  "rules": {
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "warn"
  }
}
